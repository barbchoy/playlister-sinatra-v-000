<h1>Editing <%=@song.name%></h1>
<form action="/songs/<%=@song.slug%>" method="POST">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <label>Song Name</label><input type="text" name="song_name" value="<%=@song.name%>"/>
  <label>Artist Name</label><input type="text" name="Artist Name" value="<%=@song.artist.name%>"/>
  <input type="submit" value="Save"/>
</form>
